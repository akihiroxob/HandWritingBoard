html{height: 100%;}
body{height:100%; margin:0;padding:0; font-family: "メイリオ", "Hiragino Kaku Gothic Pro", Meiryo, "ヒラギノ角ゴ Pro W3", "MS PGothic", "MS UI Gothic", Helvetica, Arial, sans-serif;color:#2F2F2F;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%;-moz-text-size-adjust:100%;text-size-adjust:100%}
article,aside,details,dialog,figcaption,figure,footer,header,hgroup,main,menu,nav,section{display:block}
article,aside,audio,canvas,dd,details,dialog,div,dl,dt,embed,figcaption,figure,footer,form,h1,h2,h3,h4,h5,h6,header,hgroup,li,main,mark,menu,nav,ol,output,p,ruby,section,summary,time,ul,video{margin:0; padding:0; font-weight:400; font-size:100%}
img{padding:0; margin:0; border:0;}
li,ol,ul{list-style:none;}
table{margin:0; padding:0; border-collapse:collapse; border-spacing:0;}
caption{text-align:left;}
code,del,input,ins,kbd,pre,samp,select,table,textarea,var{font-size:100%}
address,cite,del,dfn,em,i,ins,samp,strong,th,var{font-weight:400; font-style:normal}
address,dd,dl,dt,h1,h2,h3,li,p,ul{margin:0; padding:0; font-weight:400; font-size:100%; list-style:none; text-decoration:none;}
blockquote,q{quotes:none}
blockquote:after,blockquote:before,q:after,q:before{content:''; content:none}
textarea{ font-family:sans-serif;}
a{color:#428a67;text-decoration:none}
a:active{color:#FFF; }a:hover{color:#FFF;}a:link{color:#1A75FF;}a:visited{color:#428a67;}a:focus{outline:0; }